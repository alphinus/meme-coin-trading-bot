# Environment Configuration
# Copy this file to .env and fill in your values

# Solana RPC URLs (at least one required)
SOLANA_RPC_URL_1=https://api.mainnet-beta.solana.com
SOLANA_RPC_URL_2=https://solana-mainnet.g.alchemy.com/v2/YOUR_KEY

# Jito Block Engine URL
JITO_URL=https://mainnet.blockengine.me

# API Keys
BIRDEYE_API_KEY=your_birdeye_api_key
DEXSCREENER_API_KEY=your_dexscreener_api_key

# Twitter API (for sentiment analysis)
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret
TWITTER_BEARER_TOKEN=your_twitter_bearer_token

# Telegram (for alerts)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

# Trading Configuration
INITIAL_CAPITAL=10000
MAX_POSITION_SIZE=0.02
STOP_LOSS=0.15

# Logging
LOG_LEVEL=info
